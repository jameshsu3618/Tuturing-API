                                            <tr style="text-align: left; vertical-align: top;">
                                                <td style="padding-bottom: 32px;">
                                                    <a style="text-decoration: none;" href="https://www.tuturing.com" target="_blank"><img class="em_w150" style="display: block; max-width: 163px;" src="{{cdn_url}}/emails/tuturing_icon.png" alt="tuturing" width="163" height="48" /> </a>
                                                </td>
                                            </tr>

                                        </table>
                                    {{/if}}
                                    <div style="padding-right: 30px;line-height: 22px;font-size: 16px;color: #333333;padding-bottom: 8px;justify-content: space-between;mso-line-height-rule: exactly !important;border-collapse: collapse !important;">
                                        <table style="border-collapse: collapse; border-spacing: 0;  text-align: left; vertical-align: top; width: 100%;">

                                            <tr style="text-align: left; vertical-align: top;">
                                                <td style="padding: 0 0 20px 0;">
                                                    {{#if _confirmation}}
                                                        <span style="margin: 0;">
                                                            Thank you for booking with tuturing.
                                                            <br/>
                                                            Your reservation is booked and confirmed.
                                                        </span>
                                                        <br />
                                                    {{/if}}
                                                    {{#if _cancellation}}
                                                        <span style="margin: 0; color: #b00000;">
                                                            Your hotel reservation has been canceled
                                                                        {{#if cancellation.cancelledBy }}
                                                                            {{cancellation.cancelledBy}}
                                                                            {{#if cancellation.cancelledReason }}
                                                                                because of {{cancellation.cancelledReason}}
                                                                            {{/if}}
                                                                        {{/if}}
                                                        </span>
                                                        <br />
                                                    {{/if}}
                                                    {{#if _modification}}
                                                        <span style="margin: 0; color: #b00000;">
                                                            Here are the changes to your reservation:
                                                        </span>
                                                        <br />
                                                    {{/if}}
                                                </td>
                                            </tr>
                                        </table>
                                    </div>
                                    <div style="padding-right: 30px;line-height: 16px;font-size: 16px;font-weight: 500;color: #333333;padding-bottom: 8px;justify-content: space-between;mso-line-height-rule: exactly !important;border-collapse: collapse !important;">
                                        {{hotel_checkin}} – {{hotel_checkout}}
                                    </div>
                                    <div style="padding-right: 30px;line-height: 22px;font-size: 16px;font-weight: 500;color: #333333;padding-bottom: 8px;justify-content: space-between;mso-line-height-rule: exactly !important;border-collapse: collapse !important;">
                                        → {{city}}, {{subdivision_code}}
